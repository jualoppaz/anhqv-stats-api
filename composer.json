{
    "name": "jualoppaz/anhqv-stats-api",
    "type": "project",
    "version": "0.2.1",
    "description": "API de la aplicaci√≥n web anhqv-stats.es",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "darkaonline/l5-swagger": "7.0",
        "fideloper/proxy": "4.3.0",
        "fruitcake/laravel-cors": "1.0.5",
        "guzzlehttp/guzzle": "6.5.2",
        "laravel/framework": "7.4.0",
        "laravel/tinker": "2.3.0"
    },
    "require-dev": {
        "facade/ignition": "2.0.2",
        "fzaninotto/faker": "1.9.1",
        "mockery/mockery": "1.3.1",
        "nunomaduro/collision": "4.2.0",
        "phpunit/phpunit": "8.5.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Anhqv\\": "app/Anhqv/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "php artisan l5-swagger:generate"
        ]
    }
}
